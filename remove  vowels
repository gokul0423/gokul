#include <stdio.h>
int main() 
{
    char str[100]; // Array to hold the input string
    char result[100]; // Array to hold the resulting string without vowels
    in
    printf("Enter a string: ");
    fgets(str, sizeof(str), stdin); 

    for (i = 0; str[i] != '\0'; i++) 
    {
        if (!(str[i] == 'a' || str[i] == 'e' || str[i] == 'i' || 
              str[i] == 'o' || str[i] == 'u' ||
              str[i] == 'A' || str[i] == 'E' || str[i] == 'I' || 
              str[i] == 'O' || str[i] == 'U')) {
            result[j] = str[i];
            j++; 
        }
    }
    result[j] = '\0'; 
    printf("String without vowels: %s\n", result);
    return 0;
}
